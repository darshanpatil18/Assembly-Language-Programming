DATA SEGMENT
NUM1 DW 19BCH,
NUM2 DW 0B007H
QUO DW 1 DUP(0)
REM DW 1 DUP(0)
DATA ENDS
CODE SEGMENT
ASSUME CS:CODE,DS:DATA
START: MOV AX,DATA
MOV DS,AX
MOV AX,NUM1 ;Move the Dividend to AL
MOV AH,0H
DIV NUM2
; Zero extended for 16 bit/8 bit division
; Perform the Division operation
MOV QUO,AX ; Store the quotient to AL
MOV REM,AX ;Store the reminder to AH
MOV AH,4CH
INT 21H
CODE ENDS
END START


