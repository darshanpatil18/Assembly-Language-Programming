DATA SEGMENT
NUM1 DW 019BCH,0B007H
NUM2 DW 0CB91H,0700BH
PROD DW 4 DUP(0)
DATA ENDS

CODE SEGMENT
ASSUME CS:CODE,DS:DATA
START: MOV AX,DATA
MOV DS,AX

MOV AX,NUM1
MUL NUM2
MOV PROD,AX
MOV AX,NUM1+2
ADC AX,NUM2+2

JNC DOWN ; Check for carry
MOV PROD+4,01H
DOWN: MOV PROD+2,AX
MOV AH,4CH
INT 21H
CODE ENDS
END START


