DATA SEGMENT
NUM1 DW 019BCH,0B007H
NUM2 DW 0CB91H,0700BH
DIFF DW 4 DUP(0)
DATA ENDS

CODE SEGMENT
ASSUME CS:CODE,DS:DATA
START: MOV AX,DATA
MOV DS,AX

MOV AX,NUM1
SUB AX,NUM2
MOV DIFF,AX
MOV AX,NUM1+2
SBB AX,NUM2+2

JNC DOWN ; Check for carry
MOV DIFF+4,01H
DOWN: MOV DIFF+2,AX
MOV AH,4CH
INT 21H
CODE ENDS
END START


